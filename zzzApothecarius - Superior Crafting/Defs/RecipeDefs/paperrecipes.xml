<?xml version="1.0" encoding="UTF-8"?>

<RecipeDefs>

<!-- MAKE PAPER -->

<RecipeDef>

<defName>makepaper</defName>
<label>Make Paper</label>
<description>Makes Paper from Wood.</description>
<jobString>Making Paper.</jobString>

<workAmount>600</workAmount>

<workEffect>CutStone</workEffect>
<sustainerSoundDef>Recipe_MakeStoneBlocks</sustainerSoundDef>


<ingredients><li><filter><thingDefs>

<li>WoodLog</li>

</thingDefs></filter>

<count>15</count>

</li></ingredients>

<products>
<li>
<thingDef>paper</thingDef>
<count>5</count>
</li>
</products>

<fixedIngredientFilter>
<thingDefs>

<li>WoodLog</li>

</thingDefs>

</fixedIngredientFilter>

<skillGains>
<li>
<skill>Crafting</skill>
<xp>50</xp>
</li>
</skillGains>

</RecipeDef>

